{"primaryContentSections":[{"kind":"content","content":[{"anchor":"Overview","level":2,"type":"heading","text":"Overview"},{"type":"paragraph","inlineContent":[{"type":"image","identifier":"screenshot"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Generate a "},{"type":"codeVoice","code":"Patch"},{"type":"text","text":" from your own data model. Update your data model when the "},{"type":"codeVoice","code":"Patch"},{"type":"text","text":" changes."}]},{"type":"codeListing","syntax":"swift","code":["func simplePatch() -> Patch {","","    let generator = Node(name: \"generator\", outputs: [\"out\"])","    let processor = Node(name: \"processor\", inputs: [\"in\"], outputs: [\"out\"])","    let mixer = Node(name: \"mixer\", inputs: [\"in1\", \"in2\"], outputs: [\"out\"])","    let output = Node(name: \"output\", inputs: [\"in\"])","","    let nodes = [generator, processor, generator, processor, mixer, output]","","    let wires = Set([Wire(from: OutputID(0, 0), to: InputID(1, 0)),","                     Wire(from: OutputID(1, 0), to: InputID(4, 0)),","                     Wire(from: OutputID(2, 0), to: InputID(3, 0)),","                     Wire(from: OutputID(3, 0), to: InputID(4, 1)),","                     Wire(from: OutputID(4, 0), to: InputID(5, 0))])","","    var patch = Patch(nodes: nodes, wires: wires)","    patch.recursiveLayout(nodeIndex: 5, point: CGPoint(x: 800, y: 50))","    return patch","}","","struct ContentView: View {","    @State var patch = simplePatch()","    @State var selection = Set<NodeIndex>()","","    var body: some View {","        NodeEditor(patch: $patch, selection: $selection)","    }","}"]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"variants":[{"paths":["\/documentation\/nodeeditor"],"traits":[{"interfaceLanguage":"swift"}]}],"identifier":{"url":"doc:\/\/NodeEditor\/documentation\/NodeEditor","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"Generic node graph editor."}],"kind":"symbo